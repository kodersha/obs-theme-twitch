    width: 10px;
}

QPushButton[toolButton="true"]:last-child,
QToolButton:last-child {
    margin-right: 0;
}

QToolButton:hover {
    background-color: rgb(41, 41, 45);
    outline: none;
    border: none;
}

QToolButton:pressed {
    background-color: rgb(50, 50, 57);
    outline: none;
    border: none;
}

QToolButton:selected {
    background-color: rgb(119, 44, 232);
    outline: none;
    border: none;
}

