}
QDockWidget::float-button {
    right: 8px;
    top: 8px;
}
QDockWidget::close-button,
QDockWidget::float-button {
    border-radius: 4px;
    border: none;
    background-color: transparent;
    padding: 0;
    margin: 0;
    height: 12px;
