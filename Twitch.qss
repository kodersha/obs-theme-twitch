}

/* Dock Widget */

QWidget {

}

OBSDock {

}
OBSDock > QWidget {
    background-color: palette(dark);
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
}

OBSDock QFrame {
    background-color: palette(dark);
