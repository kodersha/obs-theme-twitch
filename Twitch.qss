}

OBSBasicStatusBar {
    margin-top: 8px;
}

/* Context Menu */

QMenu {
    background-color: rgb(31, 31, 35);
    border: 1px solid rgb(41, 41, 45);
    border-radius: 4px;
}
QMenu QWidget {
    background-color: rgb(31, 31, 35);
    border: 1px solid rgb(41, 41, 45);
    border-radius: 4px;
}
QMenu::icon {
    left: 4px;
}
QMenu::separator {
    background-color: rgb(41, 41, 45);
    height: 1px;
    margin: 2px 0;
}
QMenu::item {
    background-color: transparent;
    margin-bottom: 0;
    margin-right: 2px;
    margin-left: 2px;
    margin-top: 0;
    outline: none;
    border: none;
    padding: 8px;
    max-height: 10px;
    min-height: 10px;
}
QMenu:first-child {
    margin-top: 4px;
}
QMenu:last-child {
    margin-bottom: 4px;
}
QMenu::item:disabled {
    background-color: transparent;
    color: rgb(83, 83, 95);
}
QMenu::item:disabled:hover {
    background-color: transparent;
    color: rgb(83, 83, 95);
}
QMenu::right-arrow {
    image: url(./Twitch/expand.svg);
}

/* Group Box */

QGroupBox {
    font-family: 'Segoe UI', '.AppleSystemUIFont', Helvetica, Arial, 'MS Shell Dlg', sans-serif;
    font-size: 9pt;
    font-weight: bold;
    background-color: palette(dark);
    border-radius: 4px;
    padding-top: 32px;
    padding-left: 16px;
    padding-right: 16px;
    padding-bottom: 8px;
    margin-bottom: 0px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 8px;
    top: 8px;
}


/* ScrollBars */

::corner {
    background-color: palette(window);
    border: none;
}

QScrollBar:vertical {
    background-color: transparent;
    width: 8px;
    margin: 0;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical, QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    border: none;
    background: none;
    color: none;
}

QScrollBar:horizontal {
    background-color: transparent;
    height: 8px;
    margin: 0;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
    width: 0;
}

QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal, QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    border: none;
    background: none;
    color: none;
}

QScrollBar::handle {
    background-color: palette(button);
    margin: 2px;
    border-radius: 2px;
    border: 1px solid palette(button);
}

QScrollBar::handle:hover {
    background-color: rgb(79,83,94);
    border-color: rgb(79,83,94);
}

QScrollBar::handle:pressed {
    background-color: rgb(119, 44, 232);
