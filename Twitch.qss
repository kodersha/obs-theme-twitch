QCalendarWidget QSpinBox::down-button {subcontrol-origin: border; subcontrol-position: bottom right; width: 16px;}
QCalendarWidget QSpinBox::up-arrow { width: 10px; height: 10px; }
QCalendarWidget QSpinBox::down-arrow { width: 10px; height: 10px; }

/* Days of the Week Bar */

QCalendarWidget QWidget {
    alternate-background-color: rgb(31, 31, 35);
}

QCalendarWidget QAbstractItemView:enabled {
    background-color: palette(base);
    color: palette(text);
}

QCalendarWidget QAbstractItemView:disabled {
    color: rgb(122, 121, 122);
}

/* VirtualCam Plugin Fixes */

#VirtualProperties QWidget {
    margin-bottom: 0;
    margin-top: 0;
}

/* Disable icons on QDialogButtonBox */

QDialogButtonBox {
    dialogbuttonbox-buttons-have-icons: 0;
}

#centralwidget QFrame {
    background-color: transparent;
    padding: 0;
    margin: 0;
}
#centralwidget QWidget QPushButton {
    background-color: rgb(31, 31, 35);
    border-radius: 4px;
    color: rgb(239, 239, 241);
    outline: none;
    border: none;
    padding: 8px 12px;
}
#centralwidget QWidget QPushButton:hover {
    background-color: rgb(41, 41, 45);
    color: rgb(239, 239, 241);
}
#centralwidget QWidget QPushButton:pressed {
    background-color: rgb(50, 50, 57);
    color: rgb(239, 239, 241);
}
#centralwidget QWidget QPushButton:selected {
    background-color: rgb(119, 44, 232);
    color: rgb(247, 247, 248);
}
#centralwidget QWidget QPushButton:selected:hover {
    background-color: rgb(119, 44, 232);
    color: rgb(247, 247, 248);
}