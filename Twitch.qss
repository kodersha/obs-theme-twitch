}

* [themeID="refreshIconSmall"] {
    qproperty-icon: url(./Twitch/refresh.svg);
}

* [themeID="cogsIcon"] {
    qproperty-icon: url(./Twitch/cogs.svg);
}

#sourceInteractButton {
    qproperty-icon: url(./Twitch/interact.svg);
}

* [themeID="upArrowIconSmall"] {
    qproperty-icon: url(./Twitch/up.svg);
}

* [themeID="downArrowIconSmall"] {
    qproperty-icon: url(./Twitch/down.svg);
}

* [themeID="pauseIconSmall"] {
    qproperty-icon: url(./Twitch/media-pause.svg);
}

QToolBarExtension {
    background-color: palette(button);
    padding: 4px;
    height: 12px;
    width: 12px;
    margin-left: 0;

    qproperty-icon: url(./Twitch/dots-vert.svg);
}


/* Tab Widget */

QTabWidget::pane { /* The tab widget frame */
    border: 0;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar {
    background-color: rgb(24, 24, 27);
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 4px;
    border-top-left-radius: 4px;
}

QTabBar QToolButton {
    background-color: rgb(24, 24, 27);
    outline: none;
    border: none;
}

QTabBar::tab:top {
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 4px;
    border-top-left-radius: 4px;
}

QTabBar::tab:bottom {
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 0;
    border-top-left-radius: 0;
}

QTabBar::tab {
    background-color: rgb(24, 24, 27);
    color: rgb(83, 83, 95);
    padding: 8px 12px;
    outline: none;
    border: none;
}

QTabBar::tab:pressed {
    background-color: transparent;
    color: rgb(173, 173, 184);
}

QTabBar::tab:hover {
    background-color: transparent;
    color: rgb(132, 132, 148);
}

QTabBar::tab:selected {
    background-color: transparent;
    color: rgb(247, 247, 248);
}

QTabBar::tab:selected:hover {
    background-color: transparent;
    color: rgb(247, 247, 248);
}
QTabBar::tab:selected:pressed {
    background-color: transparent;
    color: rgb(255, 255, 255);
}

QTabBar::tab:top:selected {
    border-bottom: 0;
}

QTabBar::tab:bottom:selected {
    border-top: 0;
}

QTabBar QToolButton {
    background: palette(button);
    min-width: 16px;
    padding: 0;
}

/* ComboBox */

QComboBox,
QDateTimeEdit {
    background-color: rgb(31, 31, 35);
    border-radius: 4px;
    padding: 8px 12px;
    outline: none;
    border: none;
    min-height: 18px;
    max-height: 18px;
}

QComboBox:hover,
QComboBox:selected,
QDateTimeEdit:hover,
QDateTimeEdit:selected {
    background-color: rgb(41, 41, 45);
}

QComboBox::drop-down,
QDateTimeEdit::drop-down {
    border: none;
    width: 20px;
}

QComboBox::down-arrow,
QDateTimeEdit::down-arrow {
    qproperty-alignment: AlignTop;
    image: url(./Twitch/updown.svg);
    width: 100%;
}

QComboBox:on,
QDateTimeEdit:on {
    background-color: rgb(31, 31, 35);
}

QComboBox:editable:hover {

}

QComboBox::drop-down:editable,
QDateTimeEdit::drop-down:editable {
    border-bottom-right-radius: 4px;
    border-top-right-radius: 4px;
}

QComboBox::down-arrow:editable,
QDateTimeEdit::down-arrow:editable {
    qproperty-alignment: AlignTop;
    image: url(./Twitch/down.svg);
    width: 8%;
}

/* Textedits etc */

QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: rgb(31, 31, 35);
    border: 2px solid rgb(31, 31, 35);
    border-radius: 4px;
    padding-bottom: 6px;
    padding-right: 8px;
    padding-left: 8px;
    padding-top: 6px;
}

QLineEdit:hover,
QTextEdit:hover,
QPlainTextEdit:hover {
    background-color: rgb(41, 41, 45);
    border: 2px solid rgb(41, 41, 45);
}

QLineEdit:focus,
QTextEdit:focus,
QPlainTextEdit:focus {
    background-color: rgb(41, 41, 45);
    border: 2px solid rgb(119, 44, 232);
}

/* Spinbox and doubleSpinbox */

QSpinBox,
QDoubleSpinBox {
    background-color: rgb(31, 31, 35);
    border-radius: 4px;
    border: none;
    padding-bottom: 8px;
    padding-right: 8px;
    padding-left: 10px;
    padding-top: 8px;
    min-height: 18px;
    max-height: 18px;
}

QSpinBox:hover,
QDoubleSpinBox:hover {
    background-color: rgb(41, 41, 45);
}

QSpinBox:focus,
QDoubleSpinBox:focus {
    background-color: rgb(41, 41, 45);
}

QSpinBox::up-button, QDoubleSpinBox::up-button {
    subcontrol-origin: padding;
    subcontrol-position: top right; /* position at the top right corner */
    right: 1.5px;
    top: 6px;
    border-radius: 2px;
    border-width: 0;
    border-bottom-width: 0;
}

QSpinBox::down-button, QDoubleSpinBox::down-button {
    subcontrol-origin: padding;
    subcontrol-position: bottom right; /* position at the top right corner */
    bottom: 6px;
    right: 1.5px;
    border-radius: 2px;
    border-width: 0;
    border-top-width: 0;
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover, QDoubleSpinBox::up-button:hover, QDoubleSpinBox::down-button:hover {
    background-color: transparent;
}

QSpinBox::up-button:pressed, QSpinBox::down-button:pressed, QDoubleSpinBox::up-button:pressed, QDoubleSpinBox::down-button:pressed {
    background-color: transparent;
}

QSpinBox::up-button:disabled, QSpinBox::up-button:off, QSpinBox::down-button:disabled, QSpinBox::down-button:off {
    background-color: transparent;
}

QDoubleSpinBox::up-button:disabled, QDoubleSpinBox::up-button:off, QDoubleSpinBox::down-button:disabled, QDoubleSpinBox::down-button:off {
    background-color: transparent;
}

QSpinBox::up-arrow,
QDoubleSpinBox::up-arrow {
    qproperty-alignment: AlignTop;
    image: url(./Twitch/scroll-up.svg);
    width: 100%;
}

QSpinBox::up-arrow:hover,
QDoubleSpinBox::up-arrow:hover {
    qproperty-alignment: AlignTop;
    image: url(./Twitch/scroll-up-hover.svg);
    width: 100%;
}

QSpinBox::down-arrow,
QDoubleSpinBox::down-arrow {
    qproperty-alignment: AlignTop;
    image: url(./Twitch/scroll-down.svg);
    width: 100%;
}

QSpinBox::down-arrow:hover,
QDoubleSpinBox::down-arrow:hover {
    qproperty-alignment: AlignTop;
    image: url(./Twitch/scroll-down-hover.svg);
    width: 100%;
}


/* Controls Dock */


#controlsDock {

}
#controlsDock QPushButton {
    margin-bottom: 0;
    margin-right: 0;
    margin-left: 0;
    margin-top: 0;

    padding: 8px 12px;
    text-align: left;

    min-height: 18px;
    max-height: 18px;
}

#controlsDock QPushButton#broadcastButton {
    qproperty-icon: url(./Twitch/dots.svg);
    color: rgb(31, 31, 35);
    font-size: 1px;

    min-width: 16px;
    max-width: 16px;

    padding: 8px;
}

#controlsDock QPushButton#broadcastButton:hover {
    background-color: rgb(41, 41, 45);
    color: rgb(31, 31, 35);
}
#controlsDock QPushButton#broadcastButton:pressed {
    background-color: rgb(50, 50, 57);
    color: rgb(31, 31, 35);
}
#controlsDock QPushButton#broadcastButton:!hover:!pressed:checked,
#controlsDock QPushButton#broadcastButton:!pressed:checked {
    background-color: rgb(119, 44, 232);
    color: rgb(31, 31, 35);
}

#controlsDock QPushButton[themeID="configIconSmall"] {
    text-align: center;
}
#controlsDock QPushButton[themeID="configIconSmall"]:hover {
    background-color: rgb(41, 41, 45);
    color: rgb(239, 239, 241);
}
#controlsDock QPushButton[themeID="configIconSmall"]:pressed {
